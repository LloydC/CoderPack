<h1> Login page </h1>

<%= form_tag('/sessions', method: 'post') do %>
  <%=label_tag(:username, 'Username') %>
  <%=text_field_tag(:username) %>
   <%=label_tag(:password, 'Password') %>
  <%=password_field_tag(:password) %>
  <%=submit_tag('Login') %>
  <% end %>
  <br>
  <p><%= link_to "User Sign-up", new_user_path() %>  </p>